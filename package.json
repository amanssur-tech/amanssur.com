    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
