    "@tailwindcss/vite": "^4.1.14",
    "astro": "^5.14.1",
    "axios": "^1.12.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "svelte": "^5.39.9",
